{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi":"error",
        "quotes": ["error", "double"],
        "camelcase": "error",
        "one-var": ["error", "always"]
    }
}
